<!doctype html>
<html>
  <head>
    <title>Resume creator</title>
  </head>
  <body>
    <!-- name,dob,city,college,company,position,skills -->
    <h1> %% data[:name] %% </h1>
    <h2> %% data[:position] %% </h2>
    <h3> %% data[:company] %% </h3>

    <p><b>Date of birth</b>: %% data[:dob] %%</p>
    <p><b>City</b>: %% data[:city] %%</p>
    <p><b>College</b>: %% data[:college] %% </p>
    <p><b>Skills</b>: %% data[:skills] %%</p>
  </body>
</html>
